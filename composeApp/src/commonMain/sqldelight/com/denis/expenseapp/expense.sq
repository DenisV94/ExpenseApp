CREATE TABLE expense (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    description TEXT NOT NULL,
    amount REAL NOT NULL,
    date TEXT NOT NULL,
    category TEXT NOT NULL
);

insertExpense:
INSERT INTO expense (description, amount, date, category)
VALUES (?, ?, ?, ?);

selectAllExpenses:
SELECT * FROM expense;

selectExpenseById:
SELECT * FROM expense
WHERE id = ?;

deleteExpenseById:
DELETE FROM expense
WHERE id = ?;