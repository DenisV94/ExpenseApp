CREATE TABLE Expense (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    description TEXT NOT NULL,
    amount REAL NOT NULL,
    date TEXT NOT NULL,
    category TEXT NOT NULL
);

insertExpense:
INSERT INTO Expense (description, amount, date, category)
VALUES (?, ?, ?, ?);

selectAllExpenses:
SELECT * FROM Expense;

selectExpenseById:
SELECT * FROM Expense
WHERE id = ?;

deleteExpenseById:
DELETE FROM Expense
WHERE id = ?;